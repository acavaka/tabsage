# TabSage Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# Google API Configuration
GOOGLE_API_KEY=your-google-api-key-here
GEMINI_MODEL=gemini-2.5-flash-lite

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
VERTEX_AI_LOCATION=us-central1

# Google Cloud Credentials (optional - can use Application Default Credentials instead)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# Knowledge Graph Provider (inmemory, firestore, neo4j, neptune, pgvector)
KG_PROVIDER=inmemory

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHANNEL_ID=your-telegram-channel-id

# A2A Agent URLs (for local development)
KG_BUILDER_A2A_URL=http://localhost:8002
TOPIC_DISCOVERY_A2A_URL=http://localhost:8003
SCRIPTWRITER_A2A_URL=http://localhost:8004
GUEST_A2A_URL=http://localhost:8005
AUDIO_PRODUCER_A2A_URL=http://localhost:8006
EVALUATOR_A2A_URL=http://localhost:8007
EDITOR_A2A_URL=http://localhost:8008
PUBLISHER_A2A_URL=http://localhost:8009
